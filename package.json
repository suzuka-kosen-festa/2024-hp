{
	"name": "2024-hp",
	"type": "module",
	"version": "0.0.0",
	"packageManager": "pnpm@9.1.2",
	"description": "第59回鈴鹿高専祭 公式HP",
	"author": {
		"name": "suzuka-kosen-festa"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/suzuka-kosen-festa/2024-HP"
	},
	"sideEffects": [
		"src/styles/globals.css.ts"
	],
	"scripts": {
		"analyze": "remix vite:build --mode analyze && serve build/client",
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "NODE_ENV=production wrangler pages dev build/client",
		"test": "run-p -c test:*",
		"test:integration": "playwright test"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.9.2",
		"@remix-run/cloudflare-pages": "2.9.2",
		"@remix-run/react": "2.9.2",
		"isbot": "5.1.6",
		"react": "18.3.1",
		"react-dom": "18.3.1"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.9.1",
		"@cloudflare/workers-types": "4.20240512.0",
		"@playwright/test": "1.43.1",
		"@re-taro/eslint-config": "4.1.0",
		"@remix-run/dev": "2.9.2",
		"@types/react": "18.3.2",
		"@types/react-dom": "18.3.0",
		"@vanilla-extract/css": "1.15.2",
		"@vanilla-extract/vite-plugin": "4.0.10",
		"browserslist-to-esbuild": "2.1.1",
		"concurrently": "8.2.2",
		"eslint": "8.57.0",
		"eslint-plugin-format": "0.1.1",
		"npm-run-all2": "6.2.0",
		"playwright": "1.43.1",
		"remix-development-tools": "4.1.6",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.3",
		"terser": "5.31.0",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"vite-tsconfig-paths": "4.3.2",
		"wait-on": "7.2.0",
		"wrangler": "3.57.1"
	},
	"browserslist": [
		">0.2% and not dead, defaults"
	]
}
