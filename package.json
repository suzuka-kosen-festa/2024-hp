		"axe-playwright": "2.0.1",
		"browserslist-to-esbuild": "2.1.1",
		"concurrently": "8.2.2",
		"eslint": "8.57.0",
		"eslint-plugin-format": "0.1.1",
		"jsdom": "24.1.0",
		"npm-run-all2": "6.2.0",
		"playwright": "1.44.1",
		"plop": "4.0.1",
		"remix-development-tools": "4.2.0",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.3",
		"sirv-cli": "2.0.2",
		"storybook": "8.1.6",
		"terser": "5.31.1",
